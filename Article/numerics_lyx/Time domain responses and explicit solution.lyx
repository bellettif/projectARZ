\begin_layout Section
Structure of solutions and relaxation time
\end_layout

\begin_layout Standard
One considers the 
\begin_inset Formula $\left(\rho,v\right)$
\end_inset

 linearized system for example: 
\begin_inset Formula $\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{t}+\left(\begin{array}{cc}
\lambda_{1} & 0\\
0 & \lambda_{2}
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{x}=\left(\begin{array}{cc}
-\frac{1}{\tau} & 0\\
-\frac{1}{\tau} & 0
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)$
\end_inset

.
\end_layout

\begin_layout Subsection
Solving for 
\begin_inset Formula $\xi_{1}$
\end_inset

 along its characteristic lines
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $x\in\mathbb{R}$
\end_inset

 and 
\begin_inset Formula $F_{x}^{1}:t\rightarrow\xi_{1}\left(t,x+\lambda_{1}t\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\frac{dF_{x}^{1}}{dt}\left(t\right)=-\frac{1}{\tau}\xi_{1}\left(t,x+t\lambda_{1}\right)=-\frac{1}{\tau}F_{x}^{1}\left(t\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

Therefore, 
\begin_inset Formula $F_{x}^{1}\left(t\right)=K_{x}e^{-\frac{t}{\tau}}$
\end_inset

 and 
\begin_inset Formula 
\[
\xi_{1}\left(t,x+\lambda_{1}t\right)=\xi_{1}\left(0,x\right)e^{-\frac{t}{\tau}}
\]

\end_inset


\end_layout

\begin_layout Subsection
Solving for 
\begin_inset Formula $\xi_{2}$
\end_inset

 along its characteristic lines
\end_layout

\begin_layout Standard
Let 
\begin_inset Formula $x\in\mathbb{R}$
\end_inset

 and 
\begin_inset Formula $F_{x}^{2}:t\rightarrow\xi_{2}\left(t,x+\lambda_{2}t\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\frac{dF_{x}^{2}}{dt}\left(t\right)=-\frac{1}{\tau}\xi_{1}\left(t,x+t\lambda_{2}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

Therefore, 
\begin_inset Formula $F_{x}^{2}\left(t\right)=-\frac{1}{\tau}\int_{u=0}^{t}\xi_{1}\left(u,x+u\lambda_{2}\right)du+F_{x}^{2}\left(0\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $\xi_{1}\left(t,x+\lambda_{2}t\right)=\xi_{1}\left(t,x-\left(\lambda_{1}-\lambda_{2}\right)t+\lambda_{1}t\right)=\xi_{1}\left(0,x-\left(\lambda_{1}-\lambda_{2}\right)t\right)e^{-\frac{t}{\tau}}$
\end_inset


\begin_inset Newline newline
\end_inset

 
\begin_inset Formula 
\[
\xi_{2}\left(t,x+\lambda_{2}t\right)=-\frac{1}{\tau}\intop_{u=0}^{t}\xi_{1}\left(0,x-\left(\lambda_{1}-\lambda_{2}\right)u\right)e^{-\frac{u}{\tau}}du+\xi_{2}\left(0,x\right)
\]

\end_inset


\end_layout

\begin_layout Standard
The general expression of the solution is 
\begin_inset Formula $\left(\begin{array}{c}
\xi_{1}\left(t,x+\lambda_{1}t\right)\\
\xi_{2}\left(t,x+\lambda_{2}t\right)
\end{array}\right)=\left(\begin{array}{c}
\xi_{1}\left(0,x\right)e^{-\frac{t}{\tau}}\\
-\frac{1}{\tau}\intop_{u=0}^{t}\xi_{1}\left(0,x-\left(\lambda_{1}-\lambda_{2}\right)u\right)e^{-\frac{u}{\tau}}du+\xi_{2}\left(0,x\right)
\end{array}\right)$
\end_inset

.
\end_layout

\begin_layout Section
Linearized velocity flow system
\end_layout

\begin_layout Standard
The linearized velocity flow system is 
\begin_inset Formula 
\[
\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)_{t}+\underset{A}{\underbrace{\left(\begin{array}{cc}
v^{*}+\frac{q^{*}}{v^{*}}V^{'}\left(\frac{q^{*}}{v^{*}}\right) & 0\\
\frac{q^{*}}{v^{*}}\left(v^{*}+\frac{q^{*}}{v^{*}}V^{'}\left(\frac{q^{*}}{v^{*}}\right)\right) & v^{*}
\end{array}\right)}}\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)_{x}=\underset{B}{\underbrace{\left(\begin{array}{cc}
-\frac{\left(v^{*}\right)^{2}+q^{*}V^{'}\left(\frac{q^{*}}{v^{*}}\right)}{\left(v^{*}\right)^{2}\tau} & \frac{V^{'}\left(\frac{q^{*}}{v^{*}}\right)}{v^{*}\tau}\\
-\frac{q^{*}\left(\left(v^{*}\right)^{2}+q^{*}V^{'}\left(\frac{q^{*}}{v^{*}}\right)\right)}{\left(v^{*}\right)^{3}\tau} & \frac{q^{*}V^{'}\left(\frac{q^{*}}{v^{*}}\right)}{\left(v^{*}\right)^{2}}
\end{array}\right)}}\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)
\]

\end_inset


\end_layout

\begin_layout Subsection
Preliminary simplifications
\end_layout

\begin_layout Standard
So calculations are easier, matrices 
\begin_inset Formula $A$
\end_inset

 and 
\begin_inset Formula $B$
\end_inset

 will be expressed with 
\begin_inset Formula $\rho^{*}$
\end_inset

 and 
\begin_inset Formula $v^{*}$
\end_inset

 exclusively.
 Indeed, one has 
\begin_inset Formula $q^{*}=\rho^{*}v^{*}$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $A=\left(\begin{array}{cc}
v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right) & 0\\
\rho^{*}\left(v^{*}+\rho^{*}V'\left(\rho^{*}\right)\right) & v^{*}
\end{array}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $B=\left(\begin{array}{cc}
-\frac{v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)}{v^{*}\tau} & \frac{V^{'}\left(\rho^{*}\right)}{v^{*}\tau}\\
-\frac{\rho^{*}\left(v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)\right)}{v^{*}\tau} & \frac{\rho^{*}V^{'}\left(\rho^{*}\right)}{v^{*}\tau}
\end{array}\right)=\frac{1}{v^{*}\tau}\left(\begin{array}{cc}
-\left(v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)\right) & V^{'}\left(\rho^{*}\right)\\
-\rho^{*}\left(v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)\right) & \rho^{*}V^{'}\left(\rho^{*}\right)
\end{array}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

Let 
\begin_inset Formula $\lambda_{1}=v^{*}$
\end_inset

 and 
\begin_inset Formula $\lambda_{2}=v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)=v^{*}+\frac{q^{*}}{v^{*}}V^{'}\left(\frac{q^{*}}{v^{*}}\right)$
\end_inset

.
 
\begin_inset Formula $V^{'}\left(\rho^{*}\right)=\nicefrac{\lambda_{2}-\lambda_{1}}{\rho^{*}}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

The system simplifies as 
\begin_inset Formula 
\[
\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)_{t}+\left(\begin{array}{cc}
\lambda_{2} & 0\\
\rho^{*}\lambda_{2} & \lambda_{1}
\end{array}\right)\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)_{x}=\frac{1}{v^{*}\tau}\left(\begin{array}{cc}
-\lambda_{2} & \frac{\lambda_{2}-\lambda_{1}}{\rho^{*}}\\
-\rho^{*}\lambda_{2} & \lambda_{2}-\lambda_{1}
\end{array}\right)\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)
\]

\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula $A=P\left(\begin{array}{cc}
\lambda_{1} & 0\\
0 & \lambda_{2}
\end{array}\right)P^{-1}$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

With 
\begin_inset Formula $P=\left(\begin{array}{cc}
0 & \lambda_{2}-\lambda_{1}\\
1 & \rho^{*}\lambda_{2}
\end{array}\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

As 
\begin_inset Formula $P^{-1}BP=\frac{1}{v^{*}\tau}\left(\begin{array}{cc}
\frac{\rho^{*}\lambda_{2}}{\lambda_{1}-\lambda_{2}} & 1\\
-\frac{1}{\lambda_{1}-\lambda_{2}} & 0
\end{array}\right)\left(\begin{array}{cc}
-\lambda_{2} & \frac{\lambda_{2}-\lambda_{1}}{\rho^{*}}\\
-\rho^{*}\lambda_{2} & \lambda_{2}-\lambda_{1}
\end{array}\right)\left(\begin{array}{cc}
0 & \lambda_{2}-\lambda_{1}\\
1 & \rho^{*}\lambda_{2}
\end{array}\right)=\frac{1}{v^{*}\tau}\left(\begin{array}{cc}
-\lambda_{1} & 0\\
\frac{1}{\rho^{*}} & 0
\end{array}\right)=\left(\begin{array}{cc}
-\frac{1}{\tau} & 0\\
\frac{1}{\tau q^{*}} & 0
\end{array}\right)$
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

the system can be rewritten in the form
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset


\begin_inset Formula 
\[
\left(\begin{array}{c}
\widetilde{\xi_{1}}\\
\widetilde{\xi_{2}}
\end{array}\right)_{t}+\left(\begin{array}{cc}
\lambda_{1} & 0\\
0 & \lambda_{2}
\end{array}\right)\left(\begin{array}{c}
\widetilde{\xi_{1}}\\
\widetilde{\xi_{2}}
\end{array}\right)_{x}=\left(\begin{array}{cc}
-\frac{1}{\tau} & 0\\
\frac{1}{\tau q^{*}} & 0
\end{array}\right)\left(\begin{array}{c}
\widetilde{\xi_{1}}\\
\widetilde{\xi_{2}}
\end{array}\right)
\]

\end_inset


\begin_inset Newline newline
\end_inset

with 
\begin_inset Formula $\left(\begin{array}{c}
\widetilde{\xi_{1}}\\
\widetilde{\xi_{2}}
\end{array}\right)=P^{-1}\left(\begin{array}{c}
\tilde{v}\\
\tilde{q}
\end{array}\right)=\mbox{\left(\begin{array}{c}
 -\frac{\left(v^{*}+\rho^{*}V^{'}\left(\rho^{*}\right)\right)}{V^{'}\left(\rho^{*}\right)}\tilde{v}+\tilde{q}\\
\frac{1}{\rho^{*}V^{'}\left(\rho^{*}\right)}\tilde{v} 
\end{array}\right)}$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

Therefore, with 
\begin_inset Formula $\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)=\left(\begin{array}{c}
\widetilde{\xi_{1}}\\
-q^{*}\widetilde{\xi_{2}}
\end{array}\right)=\left(\begin{array}{c}
-\frac{\left(v^{*}+\frac{q^{*}}{v^{*}}V^{'}\left(\frac{q^{*}}{v^{*}}\right)\right)}{V^{'}\left(\frac{q^{*}}{v^{*}}\right)}\tilde{v}+\tilde{q}\\
-\frac{v^{*}}{V^{'}\left(\frac{q^{*}}{v^{*}}\right)}\tilde{v}
\end{array}\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset

The system becomes 
\begin_inset Formula 
\[
\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{t}+\left(\begin{array}{cc}
\lambda_{1} & 0\\
0 & \lambda_{2}
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{x}=\left(\begin{array}{cc}
-\frac{1}{\tau} & 0\\
-\frac{1}{\tau} & 0
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)
\]

\end_inset


\begin_inset Newline newline
\end_inset

We find the same relaxation time, as expected.
\end_layout

\begin_layout Subsection
Preliminary study of the 
\begin_inset Formula $\left(v,q\right)$
\end_inset

 system:
\end_layout

\begin_layout Subsubsection
Diagonalized system:
\end_layout

\begin_layout Standard
One obtains 
\begin_inset Formula $\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{t}+\left(\begin{array}{cc}
\lambda_{1} & 0\\
0 & \lambda_{2}
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)_{x}=\left(\begin{array}{cc}
-\frac{1}{\tau} & 0\\
-\frac{1}{\tau} & 0
\end{array}\right)\left(\begin{array}{c}
\xi_{1}\\
\xi_{2}
\end{array}\right)$
\end_inset

.
\begin_inset Newline newline
\end_inset


\begin_inset Newline newline
\end_inset

